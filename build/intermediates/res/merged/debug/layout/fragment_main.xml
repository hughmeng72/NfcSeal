<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="#F5F5F5"
    >

    <ImageButton
        android:id="@+id/fragment_main_lockImageButton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="30dp"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_lock"
        android:tag="lock" />
    
    <TextView
        android:id="@+id/fragment_main_lockImageTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/fragment_main_lockImageButton"
        android:layout_alignRight="@+id/fragment_main_lockImageButton"
        android:layout_below="@+id/fragment_main_lockImageButton"
        android:layout_marginTop="4dp"
        android:gravity="center_horizontal"
        android:text="@string/main_lock"
        android:textSize="18sp" />

    <ImageButton
        android:id="@+id/fragment_main_unlockImageButton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignTop="@+id/fragment_main_lockImageButton"
        android:layout_marginLeft="67dp"
        android:layout_toRightOf="@+id/fragment_main_lockImageButton"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_unlock"
        android:tag="unlock" />
    
    <TextView
        android:id="@+id/fragment_main_unlockImageTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/fragment_main_lockImageTextView"
        android:layout_alignBottom="@+id/fragment_main_lockImageTextView"
        android:layout_alignLeft="@+id/fragment_main_unlockImageButton"
        android:layout_alignRight="@+id/fragment_main_unlockImageButton"
        android:gravity="center_horizontal"
        android:text="@string/main_unlock"
        android:textSize="17sp" />

    <ImageButton
        android:id="@+id/fragment_main_inquiryImageButton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/fragment_main_lockImageTextView"
        android:layout_below="@+id/fragment_main_lockImageTextView"
        android:layout_marginTop="50dp"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_inquiry"
        android:tag="inquiry" />
    
    <TextView
        android:id="@+id/fragment_main_InquiryTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/fragment_main_inquiryImageButton"
        android:layout_alignRight="@+id/fragment_main_inquiryImageButton"
        android:layout_below="@+id/fragment_main_inquiryImageButton"
        android:layout_marginTop="4dp"
        android:gravity="center_horizontal"
        android:text="@string/main_inquiry"
        android:textSize="18sp" />

    <ImageButton
        android:id="@+id/fragment_main_signinImageButton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/fragment_main_unlockImageTextView"
        android:layout_alignTop="@+id/fragment_main_inquiryImageButton"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_signin2"
        android:tag="signIn" />
    
    <TextView
        android:id="@+id/fragment_main_signinTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/fragment_main_InquiryTextView"
        android:layout_alignBottom="@+id/fragment_main_InquiryTextView"
        android:layout_alignLeft="@+id/fragment_main_signinImageButton"
        android:layout_alignRight="@+id/fragment_main_signinImageButton"
        android:gravity="center_horizontal"
        android:text="@string/main_signin"
        android:textSize="18sp" />

    <ImageButton
        android:id="@+id/fragment_main_aboutMeImageButton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/fragment_main_InquiryTextView"
        android:layout_below="@+id/fragment_main_InquiryTextView"
        android:layout_marginTop="40dp"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_aboutme"
        android:tag="aboutMe" />
    
    <TextView
        android:id="@+id/fragment_main_aboutMeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/fragment_main_aboutMeImageButton"
        android:layout_alignRight="@+id/fragment_main_aboutMeImageButton"
        android:layout_below="@+id/fragment_main_aboutMeImageButton"
        android:layout_marginTop="4dp"
        android:gravity="center_horizontal"
        android:text="@string/main_aboutMe"
        android:textSize="18sp" />

    <ImageButton
        android:id="@+id/fragment_main_moreImageButton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignLeft="@+id/fragment_main_signinTextView"
        android:layout_alignTop="@+id/fragment_main_aboutMeImageButton"
        android:background="@null"
        android:scaleType="fitCenter"
        android:src="@drawable/ic_more"
        android:tag="more" />

    <TextView
        android:id="@+id/fragment_main_moreTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/fragment_main_aboutMeTextView"
        android:layout_alignBottom="@+id/fragment_main_aboutMeTextView"
        android:layout_alignLeft="@+id/fragment_main_moreImageButton"
        android:layout_alignRight="@+id/fragment_main_moreImageButton"
        android:gravity="center_horizontal"
        android:text="@string/main_more"
        android:textSize="18sp" />

</RelativeLayout>
